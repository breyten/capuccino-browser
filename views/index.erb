<!DOCTYPE html>
<html>
<head>
  <title>Cappucino Live</title>
  <link rel="stylesheet" href="/stylesheets/main.css" type="text/css">
  <script src="/javascripts/modernizr.custom.01318.js" type="text/javascript"> </script>
  <script src="/javascripts/tmpl.min.js" type="text/javascript"> </script>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- Bootstrap -->
  <link href="/stylesheets/bootstrap.min.css" rel="stylesheet" media="screen">  
</head>
<body>
  <div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/">Capuccino Live</a>
      </div>
      <div class="collapse navbar-collapse">
        <ul class="nav navbar-nav">
          <li class="active"><a href="#">Home</a></li>
          <li><a href="#about">Over</a></li>
          <li><a href="http://openstate.eu/contact">Contact</a></li>
        </ul>
      </div><!--/.nav-collapse -->
    </div>
  </div>

  <div class="container" style="margin-top: 50px; background: url(http://cappuccino.ncrv.nl/data/files/dominion/pip_cappuccino_1.jpg); ">

    <div class="row">
      <div class="col-md-4">
        <form id="radios">
          <select id="radiobox2-form-channel"  class="form-control">
          </select>
        </form>
      </div>
      <div id="radio-info" class="col-md-8">
        <img width="100" class="pull-left" />
        <h1 class="pull-right"></h1>
        <h3></h3>
        <p class="description" class="clearfix"></p>
        <p class="tags"></p>
      </div>
    </div>
    
    <div id="programme" class="row">
      <div class="col-md-1">
        <img width="100" class="pull-left" />
      </div>
      <div id="programme-info" class="col-md-3">
        <h2> <span class="glyphicon glyphicon-book"></span><span class="title"></span></h2>
        <div id="programme-start"> </div>
        <div id="programme-end"> </div>
        <div id="programme-description"> </div>
      </div>
      <div id="track" class="col-md-8">
        <h1>
          <span class="glyphicon glyphicon-stop"></span>
          <span class="artist"></span> - 
          <span class="title"></span>
        </h1>
        <p><span class="start"></span> - <span class="end"></span></p>
        <div class="player"></track>
      </div>
    </div>

    <div id="presenter" class="row">
      <div class="col-md-1">
        <img width="100" class="pull-left" />
      </div>
      <div class="col-md-3">
        <h1>Presentator</h1>
        <h2></h2>
        <div id="presenter-description"> </div>
      </div>
    </div>

  </div><!-- /.container -->
  
  <script src="/javascripts/moment.min.js" type="text/javascript"> </script>  
  <script src="/javascripts/moment-nl.js" type="text/javascript"> </script>  
  <script src="/javascripts/sprintf.min.js" type="text/javascript"> </script>  
  <script src="/javascripts/jquery-1.10.2.min.js" type="text/javascript"> </script>
  <!-- Include all compiled plugins (below), or include individual files as needed -->
  <script src="/javascripts/bootstrap.min.js" type="text/javascript"></script>
  
  <script src="/javascripts/radiobox2.js" type="text/javascript"> </script>
</body>
</html>